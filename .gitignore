# .gitignore content

# Local .terraform directories
**/.terraform/*

# .tfstate files
*.tfstate
*.tfstate.*

# Crash log files
crash.log
crash.*.log


*.tfvars
*.tfvars.json

override.tf
override.tf.json
*_override.tf
*_override.tf.json

# Ignore CLI configuration files
.terraformrc
terraform.rc

# Ignore SSH key files
echo -e "\n# Ignore SSH Keys\nmy-key\nmy-key.pub\n*.pem" >> .gitignore